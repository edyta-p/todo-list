<li id="<%= "#{dom_id(todo)}_item" %>" class="py-3">
  <%= turbo_frame_tag dom_id(todo) do %>
  <div class="flex items-center justify-between">
    <%= link_to todo.title, edit_todo_path(todo) %>
    <div class="flex items-center space-x-3">

    </div>
    <%= button_to "Delete a task", todo_path(todo), method: :delete, class: "bg-red-50 px-3 py-2 rounded inline-flex items-center justify-center text-red-600" %>
    </div>
  <% end %>
</li>
